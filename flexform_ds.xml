<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<imagepath>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagepath</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
								<wizards>
									<_PADDING>2</_PADDING>
									<link>
										<type>popup</type>
										<title>Folder Selector</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=folder</script>
										<JSopenParams>height=400,width=400,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</imagepath>
					<imagewidth>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imageheight>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<frame>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.frame</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<default></default>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</frame>
					<delay>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.delay</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<default></default>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</delay>
					<speed>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.speed</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jftcaforms/lib/class.tx_jftcaforms_tceFunc.php:&tx_jftcaforms_tceFunc->getExtSlider]]></userFunc>
								<width>400</width>
								<range>
									<lower>0</lower>
									<upper>2</upper>
								</range>
								<decimalPrecision>2</decimalPrecision>
								<emptyValue>1</emptyValue>
							</config>
						</TCEforms>
					</speed>
					<loops>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.loops</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</loops>
					<clockwise>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.clockwise</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</clockwise>
					<draggable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.draggable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</draggable>
					<throwable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.throwable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</throwable>
					<clickfree>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.clickfree</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</clickfree>
					<wheelable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.wheelable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</wheelable>

				</el>
			</ROOT>
		</settings>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.options</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>

