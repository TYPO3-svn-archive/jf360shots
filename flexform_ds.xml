<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<view>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.view</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_jf360shots_itemsProcFunc->getView</itemsProcFunc>
							</config>
						</TCEforms>
					</view>

					<imagepath>
						<TCEforms>
							<exclude>1</exclude>
							<displayCond>FIELD:view:IN:,folder</displayCond>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagepath</label>
							<config>
								<type>group</type>
								<internal_type>folder</internal_type>
								<allowed>folder</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</imagepath>
					<panoramaImage>
						<TCEforms>
							<displayCond>FIELD:view:IN:panorama</displayCond>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.panoramaImage</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>files</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>10240</max_size>
								<uploadfolder>uploads/tx_jf360shots</uploadfolder>
							</config>
						</TCEforms>
					</panoramaImage>
					<imagewidth>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imageheight>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<frame>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.frame</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<default></default>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</frame>
					<delay>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.delay</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<default></default>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</delay>
					<speed>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.speed</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jftcaforms/lib/class.tx_jftcaforms_tceFunc.php:&tx_jftcaforms_tceFunc->getExtSlider]]></userFunc>
								<width>400</width>
								<range>
									<lower>0</lower>
									<upper>2</upper>
								</range>
								<decimalPrecision>2</decimalPrecision>
								<emptyValue>1</emptyValue>
							</config>
						</TCEforms>
					</speed>
					<loops>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.loops</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</loops>
					<clockwise>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.clockwise</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</clockwise>
					<draggable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.draggable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</draggable>
					<throwable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.throwable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</throwable>
					<clickfree>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.clickfree</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</clickfree>
					<wheelable>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.wheelable</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jf360shots/lib/class.tx_jf360shots_tceFunc.php:&tx_jf360shots_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</wheelable>

				</el>
			</ROOT>
		</settings>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.options</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
					<optionsOverride>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.optionsOverride</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</optionsOverride>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>

