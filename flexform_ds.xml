<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<imagepath>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagepath</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
								<wizards>
									<_PADDING>2</_PADDING>
									<link>
										<type>popup</type>
										<title>Folder Selector</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=folder</script>
										<JSopenParams>height=400,width=400,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</imagepath>
					<imagewidth>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imageheight>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<method>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.method</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<default></default>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.from_ts</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.method.click</numIndex>
										<numIndex index="1">click</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.method.mousemove</numIndex>
										<numIndex index="1">mousemove</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</method>
					<direction>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.direction</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<default></default>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.from_ts</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.direction.forward</numIndex>
										<numIndex index="1">forward</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.direction.backward</numIndex>
										<numIndex index="1">backward</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</direction>
					<cycle>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.cycle</label>
							<config>
								<type>input</type>
								<size>3</size>
								<max>3</max>
								<default></default>
								<eval>integer</eval>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
							</config>
						</TCEforms>
					</cycle>
					<resetMargin>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.resetMargin</label>
							<config>
								<type>input</type>
								<size>3</size>
								<max>3</max>
								<default></default>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</resetMargin>
					<sensibility>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sensibility</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jftcaforms/lib/class.tx_jftcaforms_tceFunc.php:&tx_jftcaforms_tceFunc->getExtSlider]]></userFunc>
								<width>500</width>
								<range>
									<lower>0</lower>
									<upper>3</upper>
								</range>
								<decimalPrecision>2</decimalPrecision>
								<emptyValue>1</emptyValue>
							</config>
						</TCEforms>
					</sensibility>
				</el>
			</ROOT>
		</settings>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf360shots/locallang_db.xml:tt_content.pi_flexform.options</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>
